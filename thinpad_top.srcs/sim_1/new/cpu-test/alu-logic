ins_mem[3] <= 32'h00220825; // or $1, $1, $2 -> 01011101
ins_mem[4] <= 32'h302300FE; // andi $3, $1, 0x00fe -> 00000000
ins_mem[5] <= 32'h00610824; // and $1, $3, $1 -> 00000000
ins_mem[6] <= 32'h3824FF00; // xori $4, $1, 0xff00 -> 0000ff00
ins_mem[7] <= 32'h00810826; // xor $1, $4, $1 -> 0000ff00